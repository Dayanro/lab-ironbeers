<div class="container">
    <div class="row">
        <div class="col">
            {{> beerpartial randomBeer}}

            <p class="card-text"> <strong>Food Pairing</strong>
                <ul>
                    {{#each randomBeer.food_pairing}}
                    <li>{{this}}</li>
                    {{/each}}
                </ul>
            </p>
            <p class="card-text"> <strong>Beer brewers:</strong>{{randomBeer.brewers_tips}}</p>
        </div>
    </div>
</div>